<?php

/**
 *  @file
 *  Embedded Video Field provider file for BitsOnTheRun.com.
 */

function media_bitsontherun_emfield_providers($module, $provider = NULL) {
  if ($module == 'emvideo') {
    return drupal_system_listing("$provider\.inc$", drupal_get_path('module', 'media_bitsontherun') ."/providers", 'name', 0);
  }
}
